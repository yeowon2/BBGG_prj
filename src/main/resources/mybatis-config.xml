<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
  
  <configuration>
  	
  	<!-- 언더바(스네이크형식)를 낙타표기(카멜) -->
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
  
  	<typeAliases>
  		<typeAlias type="kr.ac.kopo.user.web.UserVO" alias="UserVO"/>
  		<typeAlias type="kr.ac.kopo.user.web.ProUsersVO" alias="ProUsersVO"/>
  	</typeAliases>
  
  	<mappers>
  		<mapper resource="mybatis/user.xml"/>
  		<mapper resource="mybatis/pro.xml"/>
  		<mapper resource="mybatis/login.xml"/>
  	</mappers>
  </configuration>